<body>
    <nav>
        <a class="navbar-brand" href="/">
            <img src="pics/calculator.png" class="calculator-img d-inline-block align-top " alt="gpa.jpg">
        </a>
    </nav>
    <div class="text-center">
        <form action="/signin" method="POST" class="form-signin">
            <img class="mb-4" src="pics/clown.png" alt="" width="72" height="72">
            <h1 class="h3 mb-3 font-weight-normal">SIGN IN</h1>
            <input type="email" name="username" class="signin-email form-control" placeholder="Email address" required autofocus>
            <input type="password" name="password" class="signin-password mainpassword form-control" placeholder="Password" required autofocus>
            <button class="btn btn-lg btn-primary btn-block signin-button" type="submit">Sign in</button>
            <a class="btn btn-block btn-social btn-google" href="" role="button">
                <i class="fab fa-google"></i>
                Sign In with Google
            </a>
            <p class="p-copyright mt-5 mb-3 text-muted">&copy; 2020</p>
        </form>
    </div>
</body>

</html>
